{
    "manifest_version": 2,
    "name": "Save Code, Problem",
    "description": "Save your code with problem link and than use it afterwards",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon_19.png",
        "default title": "Save Code, Problem",
        "default_popup": "html/popup.html"
    },
    "background": {
        "scripts": ["js/popup.js", "js/form.js", "js/dashboard.js"]
    },
    "permissions": ["activeTab", "tabs", "storage"]
}